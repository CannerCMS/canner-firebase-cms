  "license": "MIT",
  "devDependencies": {
    "babel-plugin-styled-components": "^1.5.1",
    "babel-plugin-transform-flow-strip-types": "^6.22.0",
    "canner-schema-loader": "^1.3.1",
    "css-loader": "^0.28.11",
    "flow-bin": "^0.75.0",
