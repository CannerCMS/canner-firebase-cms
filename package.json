    "next": "^6.1.1-canary.2",
    "query-string": "^6.1.0",
    "react": "^16.4.0",
    "react-dom": "^16.4.0",
    "react-firebaseui": "^1.2.3",
    "react-github-corner": "^2.3.0",
    "react-router": "^4.3.1",
    "slug": "^0.9.1",
    "styled-components": "^3.3.3"
